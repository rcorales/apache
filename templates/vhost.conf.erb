<VirtualHost *:<%= @port -%>>
        ServerName <%= @servername %>

        ServerAdmin webmaster@company.com
        DocumentRoot <%= @document_root %>

        Errorlog <%= @log_dir -%>/<%= @servername -%>-error.log
        CustomLog <%= @log_dir -%</<%= @servername -%>-access.log combined
</VirtualHost>        
